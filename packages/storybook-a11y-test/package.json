{
  "name": "@shopify/storybook-a11y-test",
  "version": "0.0.6",
  "license": "MIT",
  "description": "Test storybook pages with axe and puppeteer",
  "main": "index.js",
  "types": "index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "@shopify:registry": "https://registry.npmjs.org"
  },
  "author": "Shopify Inc.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shopify/quilt.git",
    "directory": "packages/storybook-a11y-test"
  },
  "bugs": {
    "url": "https://github.com/Shopify/quilt/issues"
  },
  "homepage": "https://github.com/Shopify/quilt/blob/main/packages/storybook-a11y-test/README.md",
  "dependencies": {
    "@storybook/client-api": "^6.2.9",
    "chalk": "^4.1.0",
    "p-map": "^4.0.0",
    "puppeteer": "^9.0.0"
  },
  "devDependencies": {
    "axe-core": "^4.2.0"
  },
  "peerDependencies": {
    "@storybook/addon-a11y": ">=6.0.0",
    "axe-core": ">=4.2.0"
  },
  "peerDependenciesMeta": {
    "axe-core": {
      "optional": true
    }
  },
  "files": [
    "build/*",
    "!*.tsbuildinfo",
    "index.js",
    "index.mjs",
    "index.esnext",
    "index.d.ts"
  ],
  "module": "index.mjs",
  "esnext": "index.esnext",
  "exports": {
    "./": "./",
    ".": {
      "import": "./index.mjs",
      "require": "./index.js",
      "esnext": "./index.esnext"
    }
  }
}
